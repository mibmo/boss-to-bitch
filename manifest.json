{
	"manifest_version": 2,

	"name": "__MSG_extensionName__",
    "description": "__MSG_extensionDescription__",
	"author": "Mib",
    "version": "1.0",

    "default_locale": "en",

	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
    },

    "permissions": [
        "activeTab"
    ],

	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"]
		}
	]
}
